{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "baseChores",
      "type": "blob",
      "path": "chores/baseChores.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "choresIn",
      "type": "blob",
      "path": "chores/{weekId}/chores.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ]
}
